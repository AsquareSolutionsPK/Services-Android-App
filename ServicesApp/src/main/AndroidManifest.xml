<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.asquare_solutions.www.drawerlayout">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ServicesActivity" />
        <activity android:name=".BookingFormActivity" />
        <activity android:name=".OurPartnersActivity" />
        <activity android:name=".WorkGalleryActivity" />
        <activity android:name=".AboutUsActivity" />
        <activity android:name=".ContactUsActivity" />
        <activity android:name=".PricesActivity" />
        <activity android:name=".TestimonialActivity" />
        <activity android:name=".ClientsActivity" />
        <activity android:name=".SocialActivity" />
        <activity android:name=".MethodologyActivity" />
        <activity android:name=".FAQActivity" />
        <activity android:name=".RatethisAppActivity" />
        <activity android:name=".SignInSignUpActivity" />
        <activity android:name=".SearchActivity"></activity>
    </application>

</manifest>